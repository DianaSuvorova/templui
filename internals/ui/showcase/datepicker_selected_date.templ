
package showcase

import (
	"github.com/axzilla/goilerplate/pkg/components"
	"time"
)

templ DatepickerSelectedDate() {
	<div class="w-full max-w-sm">
		@components.Datepicker(components.DatepickerProps{
			ID:          "my-datepicker",
			Name:        "selected-date",
			Placeholder: "Select a date",
			Value:       time.Now(),
		})
	</div>
}
