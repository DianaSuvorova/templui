package showcase

import "github.com/axzilla/templui/components"

templ RadioDisabled() {
	<div class="flex gap-2 items-center">
		@components.Radio("radio-disabled", "disabled", components.RadioProps{
			ID:       "radio-disabled",
			Disabled: true,
		})
		@components.Label("radio-disabled") {
			Disabled
		}
	</div>
}
