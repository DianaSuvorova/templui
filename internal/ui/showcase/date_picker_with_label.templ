package showcase

import "github.com/axzilla/templui/components"

templ DatePickerWithLabel() {
	<div class="w-full max-w-sm space-y-2">
		@components.Label("date-picker-with-label") {
			Pick a date
		}
		@components.DatePicker("date-picker-with-label",
			components.DatePickerProps{
				ID: "date-picker-with-label",
			})
	</div>
}
