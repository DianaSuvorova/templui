package showcase

import (
	"github.com/axzilla/templui/components"
	"github.com/axzilla/templui/components/label"
)

templ ToggleDisabled() {
	<div class="flex items-center gap-2">
		@components.Toggle(components.ToggleProps{
			ID:       "toggle-disabled",
			Disabled: true,
		})
		@label.Label(label.Props{
			For: "toggle-disabled",
		}) {
			Airplane Mode
		}
	</div>
}
