package showcase

import "github.com/axzilla/templui/components"

templ InputWithLabel() {
	<div class="w-full max-w-sm grid gap-2">
		@components.Label("email") {
			Email
		}
		@components.Input("email",
			components.InputProps{
				ID:          "email",
				Type:        "email",
				Placeholder: "Email",
			})
	</div>
}
